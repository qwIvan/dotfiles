#!/bin/sh
~/bin/shadowtables 1080 &
~/bin/switch-wallpaper 3286 1248  &
setxkbmap -option ctrl:swapcaps -option altwin:swap_lalt_lwin &
sudo ntpd -gq && sudo hwclock -w &

pgrep "^chrome$" || google-chrome-stable &
pgrep "^compton$" || compton --shadow-exclude 'class_g = "Sogou-qimpanel"' --shadow-exclude 'class_g = "Wine"' &
fcitx &
sudo pdnsd -c ~/parent_pdnsd.conf &
sudo pdnsd -c ~/pdnsd.conf &
sudo pdnsd -c ~/pdnsd_etc.conf &
sudo chinadns-1.3.2/src/chinadns -c chnroute.txt -s 127.0.1.1:1212,127.0.0.1:1144 -p 5355 -m &
ss-redir -c ~/ss_config.json &
adbyby &
